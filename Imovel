package Atividade4;

public class Imovel {
    
    String endereco;
    double preco;

    public Imovel(String endereco, double preco) {
        this.endereco = endereco;
        this.preco = preco;
    }
    
    
    public String mostrar(){
        String msg= "O imovel é: R$ "+preco+"\nEndereço: "+endereco;
        return msg;
    }
    
}
